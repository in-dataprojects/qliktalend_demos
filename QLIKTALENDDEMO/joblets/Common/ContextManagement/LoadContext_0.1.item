      <elementParameter field="MEMO_JAVA" name="CODE" value="/* This code makes sure that context.contextStorageType will always contain a value of type File.  If there is a user error and it is not&#xA; * File or DB, then this code resets the value to File&#xA; * Default Value is always File if a wrong value is entered&#xA; */&#xA;&#xA;if (context.contextStorageType!=null)&#xA;{&#xA;&#x9;context.contextStorageType = context.contextStorageType.trim();&#xA;}&#xA;else&#xA;{&#xA;&#x9;context.contextStorageType = &quot;File&quot;;&#xA;}&#xA;&#xA;if (!(context.contextStorageType.equals(&quot;File&quot;)||context.contextStorageType.equals(&quot;DB&quot;)))&#xA;{&#xA;&#x9;context.contextStorageType = &quot;File&quot;;&#xA;}&#xA;&#xA;&#xA;/* This code initializes the TalendEncryption encryptor with the secret key to use to encrypt or decrupt&#xA; * &#xA; * &#xA; */&#xA; &#xA;String encryptionFilePath = context.resource_directory_contextFiles + &quot;/encryption.properties&quot;;&#xA;try{&#xA;&#x9;if (context.environment.trim().length()>0)&#xA;&#x9;{&#xA;&#x9;&#x9;encryptionFilePath = context.resource_directory_contextFiles + &quot;/&quot; + context.environment.trim() +&quot;/encryption.properties&quot;;&#xA;&#x9;}&#xA;&#x9;&#xA;&#x9;TalendEncryption.initialize(encryptionFilePath);&#xA;&#xA;} catch (Exception e) {&#xA;&#x9;e.printStackTrace();&#xA;&#x9;log.error(e.toString());&#xA;&#x9;log.warn(&quot;Failed to load Secret from file &quot; + encryptionFilePath + &quot;! - Proceeding with default seed for encryptor&quot;);&#xA;}"/>
      <elementParameter field="MEMO_IMPORT" name="IMPORT" value="//import java.util.List;"/>
      <elementParameter field="TEXT" name="LABEL" value="&lt;b>__UNIQUE_NAME__&lt;/b>&lt;br>Mandatory Value check"/>
